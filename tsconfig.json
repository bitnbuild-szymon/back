{
    "compilerOptions": {
        "target": "ES2019",
        "module": "commonjs",
        "outDir": "./dist",
        "strict": true,
        "strictNullChecks": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "forceConsistentCasingInFileNames": true,
        "useUnknownInCatchVariables": false
    },
    "include": [
        "module.ts",
        "src/**/*.ts",
        "types/**/*.ts",
        "frebase-config.json"
    ]
}
